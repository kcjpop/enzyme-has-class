{
  "name": "enzyme-has-class",
  "version": "0.1.0",
  "description": "",
  "main": "src/index.jsx",
  "scripts": {
    "test": "jest --verbose",
    "start": "BABEL_ENV=dev webpack-dev-server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.0",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^16.0.0",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/))\\.test.jsx?$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "svg-inline-react": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/**/{config,store,reducers,index,actionTypes,constants}.{js,jsx}"
    ],
    "coverageDirectory": "coverage",
    "modulePaths": [
      "<rootDir>/src"
    ]
  }
